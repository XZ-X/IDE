<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.pages.EditPageController">
   <children>
      <MenuBar layoutY="23.0" prefHeight="20.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="save" mnemonicParsing="false" onAction="#onSaveClicked" text="Save" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Run">
            <items>
              <MenuItem fx:id="run" mnemonicParsing="false" onAction="#onRunClicked" text="Run" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Control">
              <items>
                <MenuItem fx:id="back" mnemonicParsing="false" text="Back" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <Label fx:id="filename" prefHeight="23.0" prefWidth="600.0" text="Label" />
      <TextArea fx:id="input" layoutX="-1.0" layoutY="284.0" prefHeight="118.0" prefWidth="316.0" promptText="Input" />
      <TextArea fx:id="content" layoutX="-1.0" layoutY="51.0" prefHeight="232.0" prefWidth="600.0" />
      <Button fx:id="runButton" layoutX="375.0" layoutY="315.0" mnemonicParsing="false" onAction="#onRunClicked" prefHeight="55.0" prefWidth="127.0" text="Run" />
   </children>
</AnchorPane>
