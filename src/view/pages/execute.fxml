<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.pages.ExecuteController">
   <children>
      <ScrollPane layoutX="78.0" layoutY="33.0" prefHeight="194.0" prefWidth="445.0">
         <content>
            <FlowPane fx:id="contentPane" prefHeight="200.0" prefWidth="427.0" />
         </content>
      </ScrollPane>
      <TextArea fx:id="inputArea" layoutX="78.0" layoutY="272.0" prefHeight="100.0" prefWidth="206.0" promptText="Input area" />
      <TextArea fx:id="outputArea" layoutX="317.0" layoutY="272.0" prefHeight="100.0" prefWidth="206.0" promptText="Output area" />
      <Button fx:id="runButton" layoutX="83.0" layoutY="237.0" mnemonicParsing="false" onAction="#refreshInput" prefHeight="27.0" prefWidth="47.0" text="Run" />
      <Button fx:id="stopButton" layoutX="163.0" layoutY="237.0" mnemonicParsing="false" onAction="#terminate" text="Stop" />
      <Button fx:id="freshButton" layoutX="394.0" layoutY="237.0" mnemonicParsing="false" text="Fresh" onMouseClicked="#refreshOutput" />
   </children>
</AnchorPane>
